{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@5.0.0-beta2",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [

        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.bundle.min.js.map",
        "dist/js/bootstrap.min.js.map",
        "scss/bootstrap.scss",
        "dist/css/bootstrap.min.css"
        
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.10.0-6",
      "destination": "wwwroot/lib/fontawesome/",
      "files": [
        "css/all.css",
        "js/all.js"

      ]

    },
    {
      "provider": "cdnjs",
      "library": "holder@2.9.7",
      "destination": "wwwroot/lib/holder",
      "files": [
        "holder.min.js"
      ]

    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "moment@2.29.1",
      "destination": "wwwroot/lib/moment",
      "files": [
        "moment.js",
        "src/lib/moment/locale.js",
        "min/moment.min.js",
        "min/moment.min.js.map",
        "min/moment-with-locales.min.js",
        "min/moment-with-locales.min.js.map"

      ]

    }
  ]
}